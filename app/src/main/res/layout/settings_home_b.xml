<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/settings_home_b"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    <RelativeLayout
            android:id="@+id/settings_home_b_frame_1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        <TextView
                android:ellipsize="end"
                android:maxLines="1"
                android:layout_toStartOf="@+id/settings_home_b_frame_2"
                android:layout_alignParentStart="true"
                android:layout_marginEnd="10dp"
                android:layout_alignParentTop="true"
                android:layout_marginTop="5dp"
                android:id="@+id/settings_home_b_text_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="30sp"/>

        <RelativeLayout
                android:layout_alignParentEnd="true"
                android:layout_alignParentTop="true"
                android:id="@+id/settings_home_b_frame_2"
                android:layout_width="100dp"
                android:layout_height="50dp">

            <ImageView
                    android:layout_centerInParent="true"
                    android:id="@+id/settings_home_b_circle"
                    android:layout_width="40dp"
                    android:layout_height="40dp"/>

        </RelativeLayout>

        <TextView
                android:layout_below="@+id/settings_home_b_frame_2"
                android:layout_marginTop="20dp"
                android:layout_centerHorizontal="true"
                android:id="@+id/settings_home_b_text_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"/>

        <ImageView
                android:layout_marginTop="20dp"
                android:layout_below="@+id/settings_home_b_text_2"
                android:layout_centerInParent="true"
                android:id="@+id/settings_home_b_icon_2"
                android:layout_width="60dp"
                android:layout_height="60dp" />

    </RelativeLayout>

    <RelativeLayout
            android:layout_marginTop="20dp"
            android:layout_below="@+id/settings_home_b_frame_1"
            android:layout_centerInParent="true"
            android:id="@+id/settings_home_b_frame_3"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        <RelativeLayout
                android:layout_alignParentStart="true"
                android:id="@+id/settings_home_b_frame_4"
                android:layout_width="60dp"
                android:layout_height="60dp">

            <ImageView
                    android:layout_centerInParent="true"
                    android:id="@+id/settings_home_b_icon_1"
                    android:layout_width="35dp"
                    android:layout_height="35dp"/>

        </RelativeLayout>

        <RelativeLayout
                android:layout_marginTop="10dp"
                android:layout_toEndOf="@+id/settings_home_b_frame_4"
                android:layout_marginStart="20dp"
                android:id="@+id/settings_home_b_frame_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content">

            <TextView
                    android:ellipsize="end"
                    android:maxLines="1"
                    android:layout_marginBottom="10dp"
                    android:layout_marginTop="10dp"
                    android:layout_marginEnd="15dp"
                    android:layout_marginStart="15dp"
                    android:layout_centerInParent="true"
                    android:id="@+id/settings_home_b_text_3"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp"/>

        </RelativeLayout>

    </RelativeLayout>

    <TextView
            android:maxLines="3"
            android:ellipsize="end"
            android:layout_below="@+id/settings_home_b_frame_3"
            android:layout_marginTop="20dp"
            android:layout_centerInParent="true"
            android:id="@+id/settings_home_b_text_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:textSize="15sp"/>

    <RelativeLayout
            android:layout_above="@+id/settings_home_b_frame_3"
            android:layout_marginBottom="-5dp"
            android:layout_centerHorizontal="true"
            android:id="@+id/settings_home_b_frame_6"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        <ListView
                android:layout_alignParentTop="true"
                android:layout_alignParentStart="true"
                android:layout_marginBottom="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:overScrollMode="never"
                android:scrollbars="none"
                android:divider="@android:color/transparent"
                android:dividerHeight="0dp"
                android:id="@+id/settings_home_b_list_view"
                android:layout_width="wrap_content"
                android:layout_height="match_parent"/>

    </RelativeLayout>

    <RelativeLayout
            android:layout_above="@+id/settings_home_b_frame_3"
            android:layout_marginBottom="-5dp"
            android:layout_centerHorizontal="true"
            android:id="@+id/settings_home_b_frame_7"
            android:layout_width="match_parent"
            android:layout_height="match_parent">

        <ImageView
                android:layout_marginStart="20dp"
                android:layout_marginTop="20dp"
                android:layout_marginEnd="20dp"
                android:layout_alignParentStart="true"
                android:layout_alignParentTop="true"
                android:id="@+id/settings_home_b_icon_5"
                android:layout_width="50dp"
                android:layout_height="50dp"/>

        <RelativeLayout
                android:layout_marginBottom="20dp"
                android:layout_marginEnd="20dp"
                android:layout_marginTop="20dp"
                android:layout_alignParentTop="true"
                android:layout_alignParentEnd="true"
                android:id="@+id/settings_home_b_frame_8"
                android:layout_width="100dp"
                android:layout_height="100dp">

            <ImageView
                    android:layout_marginBottom="20dp"
                    android:layout_marginEnd="20dp"
                    android:layout_marginStart="20dp"
                    android:layout_marginTop="20dp"
                    android:layout_centerInParent="true"
                    android:id="@+id/settings_home_b_image"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content" />

        </RelativeLayout>

        <TextView
                android:maxLines="1"
                android:ellipsize="end"
                android:layout_marginStart="20dp"
                android:layout_marginTop="10dp"
                android:layout_marginEnd="10dp"
                android:layout_below="@+id/settings_home_b_icon_5"
                android:layout_toStartOf="@+id/settings_home_b_frame_8"
                android:layout_alignParentStart="true"
                android:id="@+id/settings_home_b_text_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25sp"/>

        <TextView
                android:maxLines="1"
                android:ellipsize="end"
                android:layout_marginStart="20dp"
                android:layout_marginEnd="10dp"
                android:layout_toStartOf="@+id/settings_home_b_frame_8"
                android:layout_alignParentStart="true"
                android:layout_below="@+id/settings_home_b_text_5"
                android:id="@+id/settings_home_b_text_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"/>

        <RelativeLayout
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="20dp"
                android:layout_below="@+id/settings_home_b_text_6"
                android:layout_alignParentStart="true"
                android:id="@+id/settings_home_b_frame_9"
                android:layout_width="50dp"
                android:layout_height="50dp">

            <ImageView
                    android:layout_centerInParent="true"
                    android:id="@+id/settings_home_b_icon_3"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>

        </RelativeLayout>

        <RelativeLayout
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginStart="20dp"
                android:layout_below="@+id/settings_home_b_text_6"
                android:layout_alignParentStart="true"
                android:id="@+id/settings_home_b_frame_10"
                android:layout_width="50dp"
                android:layout_height="50dp">

            <ImageView
                    android:layout_centerInParent="true"
                    android:id="@+id/settings_home_b_icon_6"
                    android:layout_width="25dp"
                    android:layout_height="25dp"/>

        </RelativeLayout>

        <TextView
                android:alpha=".5"
                android:textAlignment="center"
                android:layout_marginEnd="5dp"
                android:layout_marginStart="5dp"
                android:layout_alignTop="@+id/settings_home_b_frame_9"
                android:layout_marginTop="5dp"
                android:layout_toEndOf="@+id/settings_home_b_frame_9"
                android:layout_toStartOf="@+id/settings_home_b_icon_4"
                android:maxLines="1"
                android:layout_centerHorizontal="true"
                android:id="@+id/settings_home_b_lines"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25sp"/>

        <ImageView
                android:layout_marginTop="20dp"
                android:layout_marginBottom="20dp"
                android:layout_marginEnd="20dp"
                android:layout_below="@+id/settings_home_b_text_6"
                android:layout_alignParentEnd="true"
                android:id="@+id/settings_home_b_icon_4"
                android:layout_width="50dp"
                android:layout_height="50dp"/>

    </RelativeLayout>

</RelativeLayout>