<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:id="@+id/settings_misc_f"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content">

    <RelativeLayout
            android:layout_alignParentStart="true"
            android:layout_alignParentTop="true"
            android:id="@+id/settings_misc_f_frame_1"
            android:layout_width="60dp"
            android:layout_height="60dp">

        <ImageView
                android:layout_centerInParent="true"
                android:id="@+id/settings_misc_f_icon_1"
                android:layout_width="30dp"
                android:layout_height="30dp" />

    </RelativeLayout>

    <RelativeLayout
            android:layout_marginStart="10dp"
            android:layout_toEndOf="@+id/settings_misc_f_frame_1"
            android:layout_alignParentTop="true"
            android:id="@+id/settings_misc_f_frame_2"
            android:layout_width="60dp"
            android:layout_height="60dp">

        <ImageView
                android:layout_centerInParent="true"
                android:id="@+id/settings_misc_f_icon_2"
                android:layout_width="30dp"
                android:layout_height="30dp" />

    </RelativeLayout>

    <ImageView
            android:layout_below="@+id/settings_misc_f_frame_1"
            android:layout_alignStart="@+id/settings_misc_f_frame_1"
            android:layout_marginStart="15dp"
            android:layout_marginTop="25dp"
            android:rotation="45"
            android:id="@+id/settings_misc_f_angle"
            android:layout_width="30dp"
            android:layout_height="30dp" />

    <RelativeLayout
            android:layout_marginTop="-25dp"
            android:layout_below="@+id/settings_misc_f_angle"
            android:layout_alignParentStart="true"
            android:id="@+id/settings_misc_f_frame_3"
            android:layout_width="match_parent"
            android:layout_height="wrap_content">

        <TextView
                android:layout_marginTop="10dp"
                android:layout_alignParentTop="true"
                android:layout_centerHorizontal="true"
                android:id="@+id/settings_misc_f_text_1"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="25sp"/>

        <TextView
                android:layout_marginEnd="15dp"
                android:layout_marginStart="15dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="15dp"
                android:layout_below="@+id/settings_misc_f_text_1"
                android:layout_centerHorizontal="true"
                android:id="@+id/settings_misc_f_text_2"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"/>

    </RelativeLayout>

    <ImageView
            android:layout_marginTop="20dp"
            android:layout_below="@+id/settings_misc_f_frame_3"
            android:layout_alignParentEnd="true"
            android:id="@+id/settings_misc_f_icon_3"
            android:layout_width="60dp"
            android:layout_height="60dp" />

    <RelativeLayout
            android:layout_marginTop="20dp"
            android:layout_below="@+id/settings_misc_f_frame_3"
            android:layout_alignParentStart="true"
            android:id="@+id/settings_misc_f_frame_4"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        <TextView
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="15dp"
                android:layout_alignParentStart="true"
                android:layout_centerInParent="true"
                android:id="@+id/settings_misc_f_text_3"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"/>

        <TextView
                android:layout_marginEnd="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="15dp"
                android:layout_toEndOf="@+id/settings_misc_f_text_3"
                android:layout_centerInParent="true"
                android:id="@+id/settings_misc_f_text_4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"/>

    </RelativeLayout>

    <RelativeLayout
            android:layout_marginStart="-100dp"
            android:layout_marginTop="20dp"
            android:layout_below="@+id/settings_misc_f_frame_4"
            android:layout_toEndOf="@+id/settings_misc_f_frame_4"
            android:id="@+id/settings_misc_f_frame_5"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content">

        <TextView
                android:layout_marginTop="15dp"
                android:layout_marginBottom="15dp"
                android:layout_marginStart="15dp"
                android:layout_alignParentStart="true"
                android:layout_centerInParent="true"
                android:id="@+id/settings_misc_f_text_5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="20sp"/>

        <TextView
                android:layout_marginEnd="15dp"
                android:layout_marginStart="5dp"
                android:layout_marginTop="2dp"
                android:layout_marginBottom="15dp"
                android:layout_toEndOf="@+id/settings_misc_f_text_5"
                android:layout_centerInParent="true"
                android:id="@+id/settings_misc_f_text_6"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp"/>

    </RelativeLayout>

    <RelativeLayout
            android:layout_alignParentTop="true"
            android:layout_alignParentEnd="true"
            android:id="@+id/settings_misc_f_frame_6"
            android:layout_width="60dp"
            android:layout_height="60dp">

        <ImageView
                android:layout_centerInParent="true"
                android:id="@+id/settings_misc_f_icon_4"
                android:layout_width="40dp"
                android:layout_height="40dp" />

    </RelativeLayout>

</RelativeLayout>